<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>AjaxServ</servlet-name>
  	<display-name>AjaxServ</display-name>
  	<description></description>
  	<servlet-class>serv.AjaxServ</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FindName</servlet-name>
  	<display-name>FindName</display-name>
  	<description></description>
  	<servlet-class>serv.FindName</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>JsonServ</servlet-name>
  	<display-name>JsonServ</display-name>
  	<description></description>
  	<servlet-class>serv.JsonServ</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FindServ</servlet-name>
  	<display-name>FindServ</display-name>
  	<description></description>
  	<servlet-class>serv.FindServ</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AjaxServ</servlet-name>
  	<url-pattern>/AjaxServ</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FindName</servlet-name>
  	<url-pattern>/FindName</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>JsonServ</servlet-name>
  	<url-pattern>/JsonServ</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FindServ</servlet-name>
  	<url-pattern>/FindServ</url-pattern>
  </servlet-mapping>
</web-app>
